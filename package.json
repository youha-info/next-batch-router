{
  "name": "next-batch-router",
  "version": "0.0.3",
  "description": "An alternative to next/router that batches push and replace calls. It allows for multiple pushes without overwriting each other.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/youha-info/next-batch-router.git"
  },
  "keywords": [
    "nextjs",
    "router",
    "batch"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/youha-info/next-batch-router/issues"
  },
  "homepage": "https://github.com/youha-info/next-batch-router",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && rollup -c",
    "prepack": "yarn build",
    "dev": "conc -n NEXT,CYPRESS 'next -p 3050' 'cypress open'",
    "ci": "next build && yarn ci:run",
    "ci:run": "conc -n NEXT,CYPRESS --kill-others 'next start -p 3050' 'yarn cypress:ci'",
    "cypress:ci":"cypress run --headless --config screenshotOnRunFailure=false,video=false"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/es/index.js",
    "default": "./dist/es/index.js"
  },
  "files": [
    "dist"
  ],
  "dependencies": {},
  "peerDependencies": {
    "next": "*",
    "react": ">=16.8"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "concurrently": "^7.5.0",
    "cypress": "^10.11.0",
    "next": "^12.2.2",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.32.1",
    "typescript": "^4.7.4"
  }
}